{
    "swagger":"2.0",
    "info":{
        "version":"0.0.1",
        "title":"Brand Geraud"
    },
    "host":"localhost:5000",
    "basePath":"/api",
    "schemes":[
        "http",
        "https"
    ],
    "consumes":[
        "application/json"
    ],
    "produces":[
        "application/json"
    ],
    "paths":{
        "/user":{
            "post":{
                "tags":[
                    "Auth"
                ],
                "description":"Returns a json on user registration",
                "parameters":[
                    {
                        "name":"New user",
                        "in":"body",
                        "required":true,
                        "description":"The User json You want to create",
                        "schema":{
                            "type":"object",
                            "properties":{
                                "name":{
                                    "type":"string",
                                    "example":"Name"
                                },
                                "username":{
                                    "type":"string",
                                    "example":"username"
                                },
                                "password":{
                                    "type":"string",
                                    "example":"Password"
                                }
                            }
                        }
                    }
                ],
                "responses":{
                    "201":{
                        "description":"created",
                        "schema":{
                            "type":"object",
                            "properties":{
                                "user":{
                                    "type":"object",
                                    "properties":{
                                        "name":{
                                            "type":"string",
                                            "example":"Name"
                                        },
                                        "username":{
                                            "type":"string",
                                            "example":"Username"
                                        },
                                        "password":{
                                            "type":"string",
                                            "example":"Password"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400":{
                        "description":"Bad request"
                    },
                    "default":{
                        "description":"Unexpected Error"
                    }
                }
            },
            "get":{
                "tags":[
                    "Users"
                ],
                "description":"Returns a json on fetch users",
                "responses":{
                    "200":{
                        "description":"List users",
                        "schema":{
                            "type":"array",
                            "properties":{
                                "type":"object",
                                "properties":{
                                    "name":{
                                        "type":"String"
                                    },
                                    "username":{
                                        "name":"String"
                                    }
                                }
                            }
                        }
                    },
                    "400":{
                        "description":"No access token provided",
                        "schema":{
                            "type":"object",
                            "properties":{
                                "error":{
                                    "type":"String"
                                }
                            }
                        }
                    },
                    "404":{
                        "description":"Users not found",
                        "schema":{
                            "type":"object",
                            "properties":{
                                "message":{
                                    "type":"Object",
                                    "example":"Users not found"
                                }
                            }
                        }
                    },
                    "default":{
                        "description":"Unexpected Error"
                    }
                }
            }
        },
        "/user/login":{
            "post":{
                "tags":[
                    "Auth"
                ],
                "description":"Returns a json web token after a Login",
                "parameters":[
                    {
                        "name":"User user",
                        "in":"body",
                        "required":true,
                        "description":"The json for login",
                        "schema":{
                            "type":"object",
                            "properties":{
                                "user":{
                                    "type":"object",
                                    "properties":{
                                        "username":{
                                            "type":"string",
                                            "example":"Username"
                                        },
                                        "password":{
                                            "type":"string",
                                            "example":"Password"
                                        }
                                    }
                                }
                            }
                        }
                    }
                ],
                "responses":{
                    "200":{
                        "description":"Ok",
                        "schema":{
                            "type":"object",
                            "properties":{
                                "user":{
                                    "type":"object",
                                    "properties":{
                                        "token":{
                                            "type":"string",
                                            "example":"eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VybmFtZSI6InVzZXJuYW0yM2hpMjMxYXNmOSIsImVtYWlsIjoibWVAZ21haWwuY29zaGEiLCJmaXJzdG5hbWUiOiJGcmllZCIsImxhc3RuYW1lIjoiTWlrZWxyaCIsImlhdCI6MTU1NTA1OTc0NX0.NfmPq_sMaD9vmdA7VH0CV2ts_6_q4eql0TiqXQVoS0w"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400":{
                        "description":"Bad request"
                    },
                    "default":{
                        "description":"Unexpected Error"
                    }
                }
            }
        },
        "/article":{
            "post":{
                "tags":[
                    "Articles"
                ],
                "description":"Returns an object with an article",
                "produces":[
                    "application/json"
                ],
                "parameters":[
                    {
                        "name":"authorization",
                        "in":"header",
                        "type":"string",
                        "required":true
                    },
                    {
                        "name":"body content",
                        "type":"object",
                        "required":true,
                        "in":"body",
                        "schema":{
                            "type":"object",
                            "properties":{
                                "title":{
                                    "type":"string",
                                    "example":"Title"
                                },
                                "content":{
                                    "type":"string",
                                    "example":"Content"
                                }
                            }
                        }
                    }
                ],
                "responses":{
                    "201":{
                        "description":"Article Added",
                        "schema":{
                            "type":"object",
                            "properties":{
                                "article":{
                                    "type":"object",
                                    "properties":{
                                        "title":{
                                            "type":"string"
                                        },
                                        "content":{
                                            "type":"string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "requestBody":{
                    "required":true,
                    "content":{
                        "application/json":{
                            "schema":{
                                "type":"object",
                                "properties":{
                                    "title":{
                                        "type":"string",
                                        "example":"Lorem Ipsum is simply dummy text"
                                    },
                                    "content":{
                                        "type":"string",
                                        "example":"Lorem Ipsum is simply dummy text of the printing   and typesetting industry. Lorem Ipsum has been the industry"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/articles/{articleID}":{
            "get":{
                "tags":[
                    "Articles"
                ],
                "description":"get article by their articleID",
                "parameters":[
                    {
                        "name":"authorization",
                        "in":"header",
                        "type":"string",
                        "required":true
                    },
                    {
                        "in":"path",
                        "name":"articleID",
                        "required":true,
                        "schema":{
                            "type":"string"
                        }
                    }
                ],
                "responses":{
                    "200":{
                        "description":"Found article",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "type":"object",
                                    "properties":{
                                        "_id":{
                                            "type":"integer",
                                            "example":1
                                        },
                                        "title":{
                                            "type":"string",
                                            "example":"Lorem Ipsum is simply dummy text"
                                        },
                                        "content":{
                                            "type":"string",
                                            "example":"Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book."
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404":{
                        "description":"No article found with the specified articleId"
                    }
                }
            },
            "delete":{
                "tags":[
                    "Articles"
                ],
                "description":"detes an article by its articleID",
                "parameters":[
                    {
                        "name":"authorization",
                        "in":"header",
                        "type":"string",
                        "required":true
                    },
                    {
                        "in":"path",
                        "name":"articleID",
                        "required":true,
                        "schema":{
                            "type":"string"
                        }
                    }
                ],
                "responses":{
                    "204":{
                        "description":"article deleted"
                    },
                    "404":{
                        "description":"no article for you to delete"
                    }
                }
            },
            "put":{
                "tags":[
                    "Articles"
                ],
                "description":"update a single article",
                "parameters":[
                    {
                        "name":"authorization",
                        "in":"header",
                        "type":"string",
                        "required":true
                    },
                    {
                        "in":"path",
                        "name":"articleID",
                        "required":true,
                        "schema":{
                            "type":"string"
                        }
                    }
                ],
                "requestBody":{
                    "required":true,
                    "content":{
                        "application/json":{
                            "schema":{
                                "type":"object",
                                "properties":{
                                    "title":{
                                        "type":"string",
                                        "example":"Lorem Ipsum is simply dummy text"
                                    },
                                    "contenr":{
                                        "type":"string",
                                        "example":"Lorem Ipsum is simply dummy text of the printing   and typesetting industry. Lorem Ipsum has been the industry"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses":{
                    "200":{
                        "description":"article updated",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "type":"object",
                                    "properties":{
                                        "id":{
                                            "type":"integer",
                                            "example":1
                                        },
                                        "title":{
                                            "type":"string",
                                            "example":"Lorem Ipsum is simply dummy text"
                                        },
                                        "slug":{
                                            "type":"string",
                                            "example":"world-class-devs-77f5d72"
                                        },
                                        "body":{
                                            "type":"string",
                                            "example":"Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book."
                                        },
                                        "createdAt":{
                                            "type":"string",
                                            "example":"2019-04-29 15:36:48.352+02"
                                        },
                                        "updatedAt":{
                                            "type":"string",
                                            "example":"2019-04-29 15:36:48.352+02"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/contact":{
            "post":{
                "tags":[
                    "Contact"
                ],
                "description":"Returns an object with a contact",
                "produces":[
                    "application/json"
                ],
                "parameters":[
                    {
                        "name":"body content",
                        "type":"object",
                        "required":true,
                        "in":"body",
                        "schema":{
                            "type":"object",
                            "properties":{
                                "name":{
                                    "type":"string",
                                    "example":"Name"
                                },
                                "email":{
                                    "type":"string",
                                    "example":"Email@email.com"
                                },
                                "phone":{
                                    "type":"string",
                                    "example":"070000000000"
                                },
                                "message":{
                                    "type":"string",
                                    "example":"Hello, This is a message from contact"
                                }
                            }
                        }
                    }
                ],
                "responses":{
                    "201":{
                        "description":"Article Added",
                        "schema":{
                            "type":"object",
                            "properties":{
                                "name":{
                                    "type":"string"
                                },
                                "email":{
                                    "type":"string"
                                },
                                "phone":{
                                    "type":"string"
                                },
                                "message":{
                                    "type":"string"
                                }
                            }
                        }
                    }
                }
            },
            "get":{
                "tags":[
                    "Contact"
                ],
                "description":"Returns an object with all contact",
                "produces":[
                    "application/json"
                ],
                "responses":{
                    "200":{
                        "description":"List contact",
                        "schema":{
                            "type":"array",
                            "properties":{
                                "type":"object",
                                "properties":{
                                    "name":{
                                        "type":"String"
                                    },
                                    "username":{
                                        "name":"String"
                                    }
                                }
                            }
                        }
                    },
                    "400":{
                        "description":"No access token provided",
                        "schema":{
                            "type":"object",
                            "properties":{
                                "error":{
                                    "type":"String"
                                }
                            }
                        }
                    },
                    "default":{
                        "description":"Unexpected Error"
                    }
                }
            }
        }
    }
}